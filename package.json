{
  "name": "foreks-pubsub-connector",
  "version": "1.4.8",
  "description": "Foreks Pubsub Connection",
  "author": "Emre Toprak <emre.toprak@foreks.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "npm run build && jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foreksorg/pubsub-connector.git"
  },
  "keywords": [
    "foreks",
    "pubsub"
  ],
  "license": "ISC",
  "private": false,
  "bugs": {
    "url": "https://github.com/foreksorg/pubsub-connector/issues"
  },
  "homepage": "https://github.com/foreksorg/pubsub-connector#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "jest": "28.1.3",
    "rollup": "^2.77.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1",
    "ts-jest": "28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
